{
	"name": "main",
	"description": "the main bench repository",
	"packages": [
		{
			"name": "bench",
			"description": "a lightweight package manager for CC",
			"version": 1.14,
			"download": {
				"bench.lua": "github://bench-cc/bench/src/bench.lua"
			},
			"install_location": "/.bench",
			"tags": ["bench", "package", "manager", "install"],
			"launch": "bench.lua"
		},
		{
			"name": "hellolib",
			"description": "a hello world library",
			"version": 1,
			"download": {
				"hellolib.lua": "github://bench-cc/bench/src/hellolib.lua"
			},
			"tags": ["hello", "world", "example", "library"]
		},
		{
			"name": "hello",
			"description": "a hello world program",
			"version": 1,
			"download": {
				"hello.lua": "github://bench-cc/bench/src/hello.lua"
			},
			"tags": ["hello", "world", "example", "program"],
			"launch": "hello.lua",
			"depends": ["hellolib"]
		},
		{
			"name": "bench-repl",
			"description": "a REPL for debugging and developing bench packages",
			"version": 1,
			"tags": ["bench", "repl", "lua", "dev", "developer", "debug", "program"],
			"download": {
				"repl.lua": "github://bench-cc/bench/src/repl.lua"
			},
			"launch": "repl.lua"
		}
	]
}
